{
  "name": "dna-frontend",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier": "npx prettier --write .",
    "format": "eslint --fix . && prettier --write .",
    "prepare": "cd .. && husky dna-frontend/.husky",
    "test": "ng test",
    "test:cov": "ng test --code-coverage",
    "coverage": "npx nyc --reporter=lcov --reporter=text npx npm run test:cov"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@ngrx/component-store": "^17.2.0",
    "@ngrx/operators": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@supabase/supabase-js": "^2.43.0",
    "@taiga-ui/addon-charts": "^3.76.0",
    "@taiga-ui/addon-commerce": "^3.76.0",
    "@taiga-ui/addon-doc": "^3.76.0",
    "@taiga-ui/addon-mobile": "^3.76.0",
    "@taiga-ui/addon-preview": "^3.76.0",
    "@taiga-ui/addon-table": "^3.76.0",
    "@taiga-ui/addon-tablebars": "^3.76.0",
    "@taiga-ui/cdk": "^3.76.0",
    "@taiga-ui/core": "^3.77.1",
    "@taiga-ui/experimental": "^3.77.1",
    "@taiga-ui/icons": "^3.76.0",
    "@taiga-ui/kit": "^3.76.0",
    "@taiga-ui/layout": "^3.76.0",
    "@taiga-ui/styles": "^3.76.0",
    "@tinkoff/ng-dompurify": "4.0.0",
    "dompurify": "3.0.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.5",
    "@angular/compiler-cli": "^17.3.0",
    "@playwright/test": "^1.44.0",
    "@types/dompurify": "3.0.5",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "playwright": "^1.44.1",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "typescript": "~5.4.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.{json,css,scss,less,md,ts,html}": [
      "prettier --write"
    ]
  }
}
